    "@tiptap/starter-kit": "^2.0.3",
    "@tiptap/vue-3": "^2.0.3",
    "@vercel/analytics": "^1.0.1",
    "axios": "^1.4.0",
    "ethers": "^5.7.0",
    "pinia": "^2.0.28",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
